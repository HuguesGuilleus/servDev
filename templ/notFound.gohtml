<!DOCTYPE html><html>
	<head>
		<meta charset="utf-8">
		<style>
			body{
				background: #f9f9fa;
				font-family: monospace;
				font-size: large;
			}
			a{
				font-size: xx-large;
			}
			a:hover{
				background:dodgerblue;
			}
		</style>
	</head>
	<body>
		<h1>404</h1>
		<p>Not found /// Non trouv√© /// No buscado</p>

		{{ $last := 0 }}
		{{range $i,$e := . }}
			{{ $last = $i }}
		{{end}}

		{{ $url := "" }}
		{{range $i,$e := .}}
			{{if ne $i $last }}
				{{ $url = print $url $e "/" }}
				<a href="{{$url}}">{{html $e}}/</a>
			{{else if ne $e ""}}
				{{ $url = print $url $e }}
				<a href="{{$url}}">{{html $e}}</a>
			{{end}}
		{{end}}
	</body>
</html>
